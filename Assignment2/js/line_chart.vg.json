{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 400,
  "title": { "text": "Global Average Suicide Rate Over Years" },
  "data": {
    "url": "https://raw.githubusercontent.com/AnabellChiew/FIT3179-testrepo/main/Assignment2/data/suicideRateByYear.csv"
  },
  "mark": {
    "type": "line",
    "point": true
  },
  "encoding": {
    "y": {
      "field": "suicide rate",
      "type": "quantitative",
      "scale": { "domain": [ 0, 20 ] }
    },
    "x": {
      "field": "year",
      "type": "ordinal",
      "axis": { "labelAngle": 0 }
    },
    "tooltip": [
      {
        "field": "suicide rate",
        "type": "quantitative",
        "format": ".2f",
        "title": "Suicide Rate"
      },
      {
        "field": "year",
        "type": "ordinal"
      }
    ]
  }
}